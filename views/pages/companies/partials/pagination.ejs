<div class="row">
    <div class="col">
        <p><%- companies.length %> of <%- resultsCount %> results</p>
        <nav>
            <ul class="pagination">
                <% for (let i = 1; i <= pagesCount; i++) { %>
                    <li class="page-item <%- page == i ? 'active' : '' %>">
                        <a href="#" class="page-link" data-page="<%- i %>">
                            <%- i %>
                        </a>
                    </li>
                <% } %>
            </ul>
        </nav>
    </div>
</div>